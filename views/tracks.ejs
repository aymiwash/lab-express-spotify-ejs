<div class="page">
  <div class="tracks_container">
    <h1 class="tracks_container_h1"><a href="/albums/<%- tracks.items[0].artists[0].id%>" >‚Üê</a> Tracks</h1>

    <ol class="tracks_results">
      <% tracks.items.forEach(tracks => { %>
      <li>
        <h3><%- tracks.name %></h3>
        <% if (tracks.preview_url) { %>
        <audio src="<%- tracks.preview_url %>" controls class="track_preview"></audio>
        <% }else{%>
        <p class="not_available">No preview available</p>
        <%} %>
      </li>
      <% }) %>
    </ol>
  </div>
</div>
