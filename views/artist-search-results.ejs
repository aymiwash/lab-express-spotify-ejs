<div id="search_result_page" class="page">
<div class="search_result_container">
  <h1 class="search_result_h1">Search result :</h1>
  <ul class="artists_result">
    <% artists.forEach(artist => { %>
    <li>
      <% if (artist.images[0]?.url) { %>
      <a href="/albums/<%- artist.id%>">
        <img
          src="<%- artist.images[0]?.url %>"
          alt="artist photo"
          class="spotifyImg"
        />
      </a>
      <% }else{%>
      <p class="not_available">No photo available</p>
      <%} %>
    <a href="/albums/<%- artist.id%>" class="title_search"> <h3 ><%- artist.name %></h3> </a> 
      <a href="/albums/<%- artist.id%>"><button class="buttons">View Albums</button></a>
    </li>
    <% }) %>
  </ul>

</div>

</div>
